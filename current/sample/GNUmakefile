ifeq ($(TOOLKIT),MAC)
./xrcdemo.app/Contents/PkgInfo: xrcdemo
	mkdir -p ./xrcdemo.app/Contents/MacOS
	mkdir -p ./xrcdemo.app/Contents/Resources
	
	
	sed -e "s/IDENTIFIER/us.ceridwen.xrcdemo/" \
	-e "s/EXECUTABLE/xrcdemo/" \
	-e "s/VERSION/1.0.0/" \
	$(WXSRCPATH)/src/mac/carbon/Info.plist.in >./xrcdemo.app/Contents/Info.plist
	
	
	echo -n "APPL????" >./xrcdemo.app/Contents/PkgInfo
	
	
	ln -f xrcdemo \
	./xrcdemo.app/Contents/MacOS/xrcdemo
endif

